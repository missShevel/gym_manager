{
  "name": "server",
  "version": "0.1.0",
  "private": "true",
  "scripts": {
    "start:db": "docker-compose up -d",
    "stop:db": "docker-compose down",
    "start": "nodemon src/index.ts",
    "start:prod": "ts-node src/index.ts",
    "eslint": "eslint --cache --ignore-path .gitignore",
    "eslint:fix": "eslint --cache --fix --ignore-path .gitignore",
    "prettier": "prettier --cache --check . --ignore-path .gitignore",
    "prettier:fix": "prettier --cache --write . --ignore-path .gitignore",
    "prepare": "husky install",
    "migration:run": "typeorm-ts-node-esm migration:run -d ./src/database/index.ts",
    "migration:undo": "typeorm-ts-node-esm migration:revert -d ./src/database/index.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@faker-js/faker": "^7.6.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "2.25.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10",
    "yup": "^0.32.11"
  }
}
